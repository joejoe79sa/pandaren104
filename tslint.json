{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
    	"quotemark": ["single"],
    	"no-console": false,
    	"max-classes-per-file": false,
    	"max-line-length": [true, 150],
    	"variable-name": [
	      true,
	      "allow-pascal-case",
	      "ban-keywords"
	    ],
	    "object-literal-shorthand": false
    },
    "rulesDirectory": []
}